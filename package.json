{
	"name": "curvy",
	"version": "0.1.0",
	"description": "Example block scaffolded with Create Block tool.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
	  "build": "wp-scripts build && postcss src/blocks/curvy/style.scss -o build/blocks/curvy/style.css",
	  "format": "wp-scripts format",
	  "lint:css": "wp-scripts lint-style",
	  "lint:js": "wp-scripts lint-js",
	  "packages-update": "wp-scripts packages-update",
	  "plugin-zip": "wp-scripts plugin-zip",
	  "start": "wp-scripts start && postcss src/blocks/curvy/style.scss -o build/blocks/curvy/style.css --watch"
	},
	"devDependencies": {
	  "@wordpress/scripts": "^27.9.0",
	  "tailwindcss": "^2.2.19",
	  "postcss": "^8.3.11",
	  "autoprefixer": "^10.4.0"
	}
  }
  